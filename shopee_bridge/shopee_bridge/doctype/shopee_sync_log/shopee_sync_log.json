{
  "doctype": "DocType",
  "name": "Shopee Sync Log",
  "module": "Shopee Bridge",
  "issingle": 1,
  "custom": 0,
  "istable": 0,
  "autoname": "",
  "fields": [
    {
      "fieldname": "sync_type",
      "fieldtype": "Select",
      "label": "Sync Type",
      "options": "orders\nreturns\nshipping\nfinance\nwebhook"
    },
    {
      "fieldname": "status",
      "fieldtype": "Select",
      "label": "Status",
      "options": "success\npartial\nfailed",
      "reqd": 1
    },
    {
      "fieldname": "from_ts",
      "fieldtype": "Datetime",
      "label": "From"
    },
    {
      "fieldname": "to_ts",
      "fieldtype": "Datetime",
      "label": "To"
    },
    {
      "fieldname": "total",
      "fieldtype": "Int",
      "label": "Total"
    },
    {
      "fieldname": "success",
      "fieldtype": "Int",
      "label": "Success"
    },
    {
      "fieldname": "failed",
      "fieldtype": "Int",
      "label": "Failed"
    },
    {
      "fieldname": "notes",
      "fieldtype": "Long Text",
      "label": "Notes"
    },
    {
      "fieldname": "payload_sample",
      "fieldtype": "Long Text",
      "label": "Payload Sample"
    },
    {
      "fieldname": "log_tail",
      "fieldtype": "Long Text",
      "label": "Recent Log Lines",
      "read_only": 1,
      "description": "Appended by write_log helper; last ~200 lines"
    },
    {
      "fieldname": "last_updated",
      "fieldtype": "Datetime",
      "label": "Last Updated",
      "read_only": 1
    }
  ]
}
